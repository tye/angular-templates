angular.module("<%= Rails.application.config.angular_templates.module_name %>", []).run(["$templateCache", function($templateCache) {
  <% AngularTemplates::TemplatingController.render_templates.each do |k,v| %>
    $templateCache.put(<%= k.to_json %>, <%= v.to_json %>)
  <% end %>
}]);
<%
depend_on Rails.root.join('app','assets','javascripts','templates')
files = Dir[Rails.root.join('app','assets','javascripts','templates','**','*')]
files.each do |file|
  depend_on file
end
%>
